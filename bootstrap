#!/usr/bin/env bash

set -e

# Environment options:
# BASH4
# OVERRIDE_MODULES_DIR

BOOTSTRAP_DIR=$(dirname "$BASH_SOURCE")
LIBRARY_DIR=$BOOTSTRAP_DIR/lib

source $LIBRARY_DIR/bootstrap.sh


function main()
{
    check_has_bash4
    assert_not_root
    assert_not_at_home
    source_library
    print_banner
    run_modules "$@"
}


main "$@"
